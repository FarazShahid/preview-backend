{
   "containerDefinitions": [ 
      { 
         "command": [
            "/bin/sh -c \"cd preview-backend && npm start\""
         ],
         "entryPoint": [
            "sh",
            "-c"
         ],
         "essential": true,
         "name": "preview-backend",
         "image": "public.ecr.aws/z5i7x6k7/preview-backend",
         "logConfiguration": { 
            "logDriver": "awslogs",
            "options": { 
               "awslogs-group" : "/ecs/fargate-task-definition",
               "awslogs-region": "us-west-1",
               "awslogs-stream-prefix": "ecs"
            }
         },
         "portMappings": [ 
            { 
               "containerPort": 80,
               "hostPort": 80,
               "protocol": "tcp"
            }
         ]
      }
   ],
   "cpu": "256",
   "executionRoleArn": "arn:aws:iam::907263135169:role/ecsTaskExecutionRole",
   "family": "fargate-task-definition",
   "memory": "512",
   "networkMode": "awsvpc",
   "requiresCompatibilities": [ 
       "FARGATE" 
    ]
}